{
  "name": "react-bootstrap-super-textbox",
  "version": "0.0.2",
  "private": false,
  "description": "A textbox that can act as a drop down list with autocomplete.",
  "main": "dist/exports.js",
  "typings": "dist/exports.d.ts",
  "browser": {
    "dist/exports.js": "./dist/exports.js"
  },
  "keywords": [
    "react",
    "props"
  ],
  "scripts": {
    "build": "webpack --progress --colors & yarn link",
    "test": "jest --env=jsdom",
    "test:watch": "yarn test -- --watch",
    "coverage": "yarn test -- --coverage --no-cache",
    "coverage:watch": "yarn run coverage -- --watch",
    "start": "webpack-serve --config webpack.config.dev.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@types/jest": "^23.3.13",
    "@types/node": "^9.4.0",
    "@types/react": "^16.7.20",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.1",
    "browserify": "^16.2.3",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react-app-polyfill": "^0.2.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "src/lib/.*\\.test\\.(ts|tsx)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
